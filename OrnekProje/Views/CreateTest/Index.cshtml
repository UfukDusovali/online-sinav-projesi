@model IEnumerable<OrnekProje.Models.InformationQuestion>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Sınav Listesi Sayfası</h2>

<p>
    @Html.ActionLink("Test oluştur", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Başlık")
        </th>

        <th>
            @Html.DisplayName("Oluşturma Tarihi")
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreateDate)
        </td>
   
        <td>
            @Html.ActionLink("Soru çöz", "Exam",new{ id=item.Id}) |
            @Html.ActionLink("Düzenle", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detay", "Details", new { id=item.Id }) |
            @Html.ActionLink("Sil", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
